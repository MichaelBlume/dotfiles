#!/usr/bin/python
import sys, time

def tl():
    s = sys.stdin.readline().rstrip()
    if not s:
        sys.exit()
    print s
    return s

def tls():
    while True:
        s = tl()
        if not s.startswith(' '):
            return s

def main(context_below=1, sleep_time=1):
    while True:
        if "ERROR" in tls():
            for i in range(context_below):
                tls()
            time.sleep(sleep_time)

if __name__  == '__main__':
    main()
